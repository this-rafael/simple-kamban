{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/simple-kambam/src",
  "monorepo": true,
  "root": "apps/simple-kambam",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/simple-kambam/tsconfig.app.json"
  },
  "projects": {
    "simple-kambam": {
      "type": "application",
      "root": "apps/simple-kambam",
      "entryFile": "main",
      "sourceRoot": "apps/simple-kambam/src",
      "compilerOptions": {
        "tsConfigPath": "apps/simple-kambam/tsconfig.app.json"
      }
    },
    "tasks": {
      "type": "application",
      "root": "apps/tasks",
      "entryFile": "main",
      "sourceRoot": "apps/tasks/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tasks/tsconfig.app.json"
      }
    },
    "errors": {
      "type": "library",
      "root": "libs/errors",
      "entryFile": "index",
      "sourceRoot": "libs/errors/src",
      "compilerOptions": {
        "tsConfigPath": "libs/errors/tsconfig.lib.json"
      }
    },
    "nestjs-helpers": {
      "type": "library",
      "root": "libs/nestjs-helpers",
      "entryFile": "index",
      "sourceRoot": "libs/nestjs-helpers/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nestjs-helpers/tsconfig.lib.json"
      }
    },
    "global-types": {
      "type": "library",
      "root": "libs/global-types",
      "entryFile": "index",
      "sourceRoot": "libs/global-types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/global-types/tsconfig.lib.json"
      }
    },
    "shared-env": {
      "type": "library",
      "root": "libs/shared-env",
      "entryFile": "index",
      "sourceRoot": "libs/shared-env/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared-env/tsconfig.lib.json"
      }
    },
    "kafka-topics": {
      "type": "library",
      "root": "libs/kafka-topics",
      "entryFile": "index",
      "sourceRoot": "libs/kafka-topics/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kafka-topics/tsconfig.lib.json"
      }
    },
    "api-exceptions": {
      "type": "library",
      "root": "libs/api-exceptions",
      "entryFile": "index",
      "sourceRoot": "libs/api-exceptions/src",
      "compilerOptions": {
        "tsConfigPath": "libs/api-exceptions/tsconfig.lib.json"
      }
    },
    "handler-kafka": {
      "type": "library",
      "root": "libs/handler-kafka",
      "entryFile": "index",
      "sourceRoot": "libs/handler-kafka/src",
      "compilerOptions": {
        "tsConfigPath": "libs/handler-kafka/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    }
  }
}